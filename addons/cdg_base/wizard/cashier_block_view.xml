<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cashier_block_form" model="ir.ui.view">
            <field name="name">cashier block form</field>
            <field name="model">cashier.block</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="block_num"/>
                        </group>
                    </sheet>
                    <footer>
                        <button type="object" name="cashier_block_num" string="確定" class="oe_highlight"/>
                        <button special="cancel" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>
        <act_window id="action_cashier_block" name="收費員捐款者名冊" res_model="cashier.block"
                    view_mode="form" target="new" multi="False"/>
    </data>


</odoo>