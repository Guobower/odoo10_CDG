<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="cashier_consultant_form" model="ir.ui.view">
            <field name="name">cashier consultant form</field>
            <field name="model">cashier.consultant</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="block_num"/>
                        </group>
                    </sheet>
                    <footer>
                        <button type="object" name="cashier_consultant_num" string="確定" class="oe_highlight"/>
                        <button special="cancel" string="取消"/>
                    </footer>
                </form>
            </field>
        </record>
        <act_window id="action_cashier_consultant" name="收費員顧問名冊" res_model="cashier.consultant"
                    view_mode="form" target="new" multi="False"/>
    </data>
</odoo>